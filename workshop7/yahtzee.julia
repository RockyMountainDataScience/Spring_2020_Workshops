function yahtzee_sim_julia(num_sim)
  count_result = 0
  
  for i = 1:num_sim
    die_roll = rand(1:5, 5)
    println(die_roll)
    if length(unique(die_roll)) == 1
      count_result = count_result + 1
    end
  end
    
  return count_result
end

yahtzee_result = yahtzee_sim_julia(10000000)
println(yahtzee_result)